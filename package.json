{
  "type": "module",
  "private": true,
  "packageManager": "pnpm@7.12.0",
  "scripts": {
    "dev": "vite",
    "dev:force": "vite --force",
    "dev:staging": "vite --mode staging",
    "build": "vite build",
    "lint": "eslint . --fix",
    "typecheck": "vue-tsc --noEmit",
    "preview": "vite preview",
    "test": "vitest",
    "up": "taze major -I",
    "prepare": "husky install"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.1.0",
    "@formily/core": "^2.2.20",
    "@formily/element-plus": "1.0.0-beta.8",
    "@formily/vue": "^2.2.20",
    "@oit/utils": "^0.1.0",
    "@uxuip/element-plus-query": "0.0.1",
    "@vue/composition-api": "^1.7.1",
    "@vueuse/core": "^9.13.0",
    "@vueuse/integrations": "^9.13.0",
    "animate.css": "^4.1.1",
    "axios": "^1.3.4",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.11.7",
    "element-plus": "^2.3.1",
    "lodash-es": "^4.17.21",
    "mime": "^3.0.0",
    "pinia": "^2.0.33",
    "qs": "^6.11.1",
    "vue": "^3.2.47",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.4.4",
    "@iconify/json": "^2.2.37",
    "@intlify/unplugin-vue-i18n": "^0.9.3",
    "@oit/eslint-config-vue-ts": "^0.4.7",
    "@types/crypto-js": "^4.1.1",
    "@types/mime": "^3.0.1",
    "@types/node": "^18.15.5",
    "@unocss/reset": "^0.50.6",
    "@uxuip/eslint-config": "^0.1.7",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vue/test-utils": "^2.3.2",
    "commitlint": "^17.4.4",
    "cross-env": "^7.0.3",
    "eslint": "^8.36.0",
    "husky": "^8.0.3",
    "jsdom": "^21.1.1",
    "lint-staged": "^13.2.0",
    "sass": "^1.59.3",
    "taze": "^0.9.1",
    "typescript": "^4.9.5",
    "unocss": "^0.50.6",
    "unplugin-auto-import": "^0.15.1",
    "unplugin-icons": "^0.15.3",
    "unplugin-vue-components": "^0.24.1",
    "vite": "^4.2.1",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-pages": "^0.28.0",
    "vite-plugin-vue-layouts": "^0.8.0",
    "vitest": "^0.29.7",
    "vue-tsc": "^1.2.0"
  },
  "lint-staged": {
    "*.{vue,js,ts,json}": [
      "cross-env NODE_ENV=production npm run lint"
    ]
  },
  "commitlint": {
    "extends": "@commitlint/config-conventional"
  },
  "eslintConfig": {
    "extends": "@uxuip",
    "rules": {
      "@typescript-eslint/prefer-ts-expect-error": "off",
      "@typescript-eslint/ban-ts-comment": "off",
      "vue/component-name-in-template-casing": [
        "error",
        "PascalCase",
        {
          "registeredComponentsOnly": false
        }
      ],
      "vue/attributes-order": [
        "error",
        {
          "order": [
            "DEFINITION",
            "LIST_RENDERING",
            "UNIQUE",
            "CONDITIONALS",
            "RENDER_MODIFIERS",
            "GLOBAL",
            "SLOT",
            "TWO_WAY_BINDING",
            "OTHER_DIRECTIVES",
            "OTHER_ATTR",
            "EVENTS",
            "CONTENT"
          ],
          "alphabetical": true
        }
      ],
      "@typescript-eslint/no-unused-vars": "warn",
      "no-debugger": "warn",
      "no-restricted-syntax": "warn"
    }
  }
}
